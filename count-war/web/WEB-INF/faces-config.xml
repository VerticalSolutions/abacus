<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>dashboard</from-outcome>
            <to-view-id>/pages/dashboard.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>accounts</from-outcome>
            <to-view-id>/pages/accounts.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
         <navigation-case>
            <from-outcome>reports</from-outcome>
            <to-view-id>/pages/reports/reports.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>contacts</from-outcome>
            <to-view-id>/pages/contacts/contacts.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>settings</from-outcome>
            <to-view-id>/pages/settings/settings.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/dashboard.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>contacts</from-outcome>
            <to-view-id>/pages/contacts/contacts.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/contacts/contacts.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/pages/contacts/contactEdit.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/contacts/contactEdit.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>list</from-outcome>
            <to-view-id>/pages/contacts/contacts.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/settings/settings.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>organization</from-outcome>
            <to-view-id>/pages/settings/organization.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/accounts.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>ledger</from-outcome>
            <to-view-id>/pages/ledger/generalledger.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/accounts.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>manual_journals</from-outcome>
            <to-view-id>/pages/ledger/manualjournal.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/ledger/manualjournal.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/pages/ledger/entry.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/ledger/generalledger.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/pages/ledger/entry.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/reports/reports.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>trialbalance</from-outcome>
            <to-view-id>/pages/reports/trialbalance.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>contactsBean</managed-bean-name>
        <managed-bean-class>count.web.managed.ContactsSearchBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>contactsTabBean</managed-bean-name>
        <managed-bean-class>count.web.managed.ContactsTabBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>accountsBean</managed-bean-name>
        <managed-bean-class>count.web.managed.GLAccountsSearchBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>contactEditBean</managed-bean-name>
        <managed-bean-class>count.web.managed.ContactEditBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>organizationEditBean</managed-bean-name>
        <managed-bean-class>count.web.managed.OrganizationEditBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>dataExportBean</managed-bean-name>
        <managed-bean-class>count.web.managed.DataExportBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>entriesBean</managed-bean-name>
        <managed-bean-class>count.web.managed.GLEntriesSearchBean</managed-bean-class>
    
        <managed-bean-scope>view</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>manualJournalTabsBean</managed-bean-name>
        <managed-bean-class>count.web.managed.ManualJournalTabsBean</managed-bean-class>
    
        <managed-bean-scope>view</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>ledgerTabsBean</managed-bean-name>
        <managed-bean-class>count.web.managed.GeneralLedgerTabsBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>entryEditBean</managed-bean-name>
        <managed-bean-class>count.web.managed.GLEntryEditBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
</faces-config>
